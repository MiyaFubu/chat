<div class="page-header">
<h1>ChatRoom一覧</h1>
</div>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>room_id</th>
      <th>room_name</th>
    </tr>
  </thead>
<% @chat_rooms.each do |chat_room| %>
  <tbody>
    <td><%= chat_room.id %></td>
    <td><%= chat_room.chat_name %></td>
    <td><%= link_to 'in', chat_room_path(chat_room), class: 'btn btn-success'%></td>
  </tbody>
<% end %>
</table>
